<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="MySQL-auto-tests" parallel="tests" thread-count="1">
    <test name="MySQL_DQL_Test" parallel="methods" thread-count="1">
        <!-- name中的值为需要测试的具体类名，格式：包名.类名 -->
        <classes>
            <class name="io.dingodb.mysqltest.TestDQLMySQL"></class>
        </classes>
    </test>
    
    <test name="MySQL_BVT_Test" parallel="methods" thread-count="1">
        <!-- name中的值为需要测试的具体类名，格式：包名.类名 -->
        <classes>
            <class name="io.dingodb.mysqltest.TestMySQLBVT"></class>
        </classes>
    </test>
</suite>
